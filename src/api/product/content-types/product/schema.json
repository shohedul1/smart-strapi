{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "isNew": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "oldPrice": {
      "required": true,
      "type": "integer"
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "category": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "rating": {
      "type": "integer",
      "required": true
    },
    "quantity": {
      "type": "integer"
    }
  }
}
